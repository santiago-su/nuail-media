<div class="all-project-images-list">
  <ul class="project-list">
    <div class="mdl-grid project-list-grid mdl-grid--no-spacing">
      <% photos.each do |project| %>
        <div class="mdl-cell mdl-cell--4-col list-image">
          <li>
            <div id="<%= project.id %>" class="list-project-container">
              <%= cl_image_tag(project.photo_images.last.file_url,:crop => :fill, :height => 180, class: 'list-image') %>
              <div class="project-info">
                <h4><%= project.name %></h4>
                <p><%= project.description %></p>
              </div>
            </div>
          </li>
        </div>
      <% end %>
    </div>
  </ul>
</div>
